{
  "expo": {
    "name": "VSNApp",
    "slug": "vsn-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/iconVSN.png",
    "splash": {
      "image": "./assets/splashVSN.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
      
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/iconVSN.png",
        "backgroundColor": "#FFFFFF"
      },
      "useNextNotificationsApi": true
    },
    "web": {
      "favicon": "./assets/iconVSN.png"
    }
  }
}
